{
  "name": "@pansy/hooks",
  "version": "0.0.1",
  "description": "react hooks",
  "main": "index.js",
  "repository": "git@github.com:pansyjs/hooks.git",
  "author": "Pansy Team",
  "license": "MIT",
  "scripts": {
    "bootstrap": "yarn || npm i",
    "build": "pansy",
    "test": "walrus test",
    "prettier": "walrus prettier",
    "lint": "walrus lint",
    "lint:commit": "walrus commitlint --env HUSKY_GIT_PARAMS",
    "prerelease": "yarn build",
    "release": "walrus release"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "dependencies": {
    "@pansy/is-boolean": "^0.3.3"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^3.2.1",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@walrus/cli": "^0.4.1",
    "@walrus/pansy": "^0.3.0",
    "@walrus/plugin-release": "^1.3.4",
    "globby": "^10.0.1",
    "husky": "^3.1.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-test-renderer": "^16.12.0",
    "typescript": "^3.7.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier",
      "commit-msg": "yarn lint:commit"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
